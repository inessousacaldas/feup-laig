<transformations>

	<transformation id = "iceWorld">
		<translate x="70" y="10" z="-150" />
		<rotate axis="y" angle="-90" />
	</transformation>
	
	<transformation id = "darkWorldTrans">
		<translate x="90" y="-20" z="-40" />
	</transformation>
	
