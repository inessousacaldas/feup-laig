		<ambient  r="1.0" g="0.4" b="0.0" a="1.0" />
		<diffuse  r="1.0" g="0.4" b="0.0" a="1.0" />
		<specular r="1.0" g="1.0" b="0.2" a="1.0" />
		<shininess value="1" />
	</material>
	
	<material id="snowMat">
		<emission r="0" g="0" b="0" a="0" />
		<ambient  r="1.0" g="0.98" b="0.94" a="1.0" />
		<diffuse  r="1.0" g="0.98" b="0.94" a="1.0" />
		<specular r="1.0" g="1.0" b="0.2" a="1.0" />
		<shininess value="1" />
	</material>
	
