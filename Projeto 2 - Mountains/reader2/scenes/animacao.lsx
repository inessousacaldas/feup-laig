			<SCALE sx="9" sy="9" sz="9" />
			<TRANSLATION x="-0.5" y="1" z="-0.5" />
            <DESCENDANTS>
                <DESCENDANT id="plataform1" />
                <DESCENDANT id="plataform2" />
